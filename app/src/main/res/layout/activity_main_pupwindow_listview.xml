<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/pullwindow_background"
    android:orientation="vertical">

   <ListView
       android:id="@+id/acticty_main_popwondow_listview"
       android:layout_marginTop="@dimen/popwindow_listvewtop"
       android:divider="#00000000"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"></ListView>
    <TextView
        android:id="@+id/acticty_main_popwondow_listview_returnhome"
        android:textColor="#fff"
        android:textSize="@dimen/returnhomesize"
        android:text="@string/pupwindowlistvewreturnhome"
        android:layout_marginLeft="@dimen/returnhomeleft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginTop="88px"
        android:id="@+id/acticty_main_popwondow_listview_exit"
        android:textColor="#fff"
        android:textSize="@dimen/exitsize"
        android:text="@string/exit"
        android:layout_marginLeft="@dimen/exitleft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


</com.zhy.autolayout.AutoLinearLayout>